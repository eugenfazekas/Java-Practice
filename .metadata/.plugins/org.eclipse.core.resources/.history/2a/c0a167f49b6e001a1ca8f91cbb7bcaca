package teszt;

public abstract class Sikidom {

	public static  int  szam = 1;
	
	private double kerulet;
	protected abstract double keruletszamit();
	public double kerulet() {
		if(kerulet == 0 ) kerulet = keruletszamit();
		return kerulet;
	}
	
	private double terulet;
	protected abstract double teruletszamit();
	public double terulet() {
		
		if(terulet == 0 ) terulet = teruletszamit();
		return terulet;
	}

	
}
